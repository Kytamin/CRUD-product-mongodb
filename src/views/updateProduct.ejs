<div class="page-heading">

    <h1>Register a new product</h1>
   
   </div>
   
   <form action="/product/update?id=<%=productSearch.id%>" method="POST" enctype="multipart/form-data">
   
    <div class="control-group">
   
    <div class="form-group floating-label-form-group controls">
   
    <label>Name</label>
   
    <input type="text" class="form-control" id="name" name="name" value="<%=productSearch.name%>">
   
    </div>
   
    </div>
   
    <div class="control-group">
   
    <div class="form-group floating-label-form-group controls">
   
    <label>Price</label>
   
    <input type="number" class="form-control" id="price" name="price" value="<%=productSearch.price%>">
   
    </div>
   
    </div>
   
    <div class="control-group">
   
    <div class="form-group floating-label-form-group controls">
   
    <label>Producer</label>
   
    <input type="text" class="form-control" id="producer" name="producer" value="<%=productSearch.producer%>">
   
    </div>
   
    </div>
   
    <div class="control-group">
   
    <div class="form-group floating-label-form-group controls">
   
    <label>Avatar</label>
   
    <input type="file" class="form-control" id="avatar" name="avatar" value="<%=productSearch.avatar%>">
   
    </div>
   
    </div>
   
    <div class="form-group">
   
    <button type="submit" class="btn btn-primary" >Update</button></div>
   
   </form>
   <%if(textproduct){%>
    <p>"the Product is Existed"</p>
<%}%>